{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "types/*.d.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "arrow-parens": false
  },
  "rulesDirectory": []
}
